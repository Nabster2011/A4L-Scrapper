{
    "manifest_version": 2,
    "name": "A4L Scrapper",
    "version": "1.0.0.1",
    "description": "Scrap clients' information",
    "icons": {
        "128": "images/icon128.png",
        "48": "images/icon48.png",
        "16": "images/icon16.png"
    },
    "page_action": {
        "default_icon": "images/icon16.png",
        "default_popup": "popup.html",
        "default_title": "A4L Scrapper"
    },
    "background": {
        "scripts": ["backend.js", "actionHandler.js"],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["https://myagedcare-assessorportal.dss.gov.au/?securemtoken*", "https://myagedcare-assessorportal.health.gov.au/?securemtoken*"],
            "js": ["actionHandler.js", "jquery-3.3.1.min.js", "contentScript.js"],
            "css": ["css/form.css"]
        }
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "https://myagedcare-assessorportal.dss.gov.au/?securemtoken*",
		"https://myagedcare-assessorportal.health.gov.au/?securemtoken*",
        "https://localhost:44381/*", "https://neptune.aspire4life.com.au/", "https://test-neptune.aspire4life.com.au/", "https://train-neptune.aspire4life.com.au/"
    ],
    "web_accessible_resources":[
        "images/aspire4life.png"
      ]
  }